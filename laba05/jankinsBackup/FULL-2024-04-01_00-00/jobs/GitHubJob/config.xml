  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.BatchFile>
      <command>@echo off&#xd;
&#xd;
rem Активация pyenv и установка версии Python&#xd;
call %USERPROFILE%\.bashrc&#xd;
pyenv versions&#xd;
pyenv global 3.6.9&#xd;
&#xd;
rem Создание виртуальной среды&#xd;
python -m venv myenv&#xd;
call myenv\Scripts\activate&#xd;
&#xd;
rem Установка зависимостей&#xd;
echo Install requirements&#xd;
pip install -r requirements.txt&#xd;
pip install pytest-cov&#xd;
&#xd;
rem Запуск тестов с измерением покрытия кода&#xd;
echo Run tests&#xd;
pytest --cov=main utests --junitxml=xmlReport\output.xml&#xd;
&#xd;
rem Генерация отчета о покрытии кода в формате XML&#xd;
echo Generating coverage report&#xd;
python -m coverage xml&#xd;
&#xd;
rem Деактивация виртуальной среды&#xd;
echo Deactivating virtual environment&#xd;
call myenv\Scripts\deactivate&#xd;
&#xd;
rem Возврат к системной версии Python&#xd;
pyenv global system&#xd;
&#xd;
echo Done</command>
      <configuredLocalRules/>
    </hudson.tasks.BatchFile>
  </builders>
